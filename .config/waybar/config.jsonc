[
  {
    "output": "HDMI-A-1",
    "layer": "top",
    "position": "top",
    "height": 30,
    "width": 1920,
    "spacing": 1,
    "modules-left": ["hyprland/workspaces", "tray", "hyprland/submap"],
    "modules-right": [
      "network",
      "custom/workspaces-mode",
      "idle_inhibitor",
      "disk#ssd",
      "temperature",
      "cpu",
      "memory",
      "custom/audio-output",
      "pulseaudio",
      "clock"
    ],
    "hyprland/workspaces": {
      "format": "{id}",
      "cursor": true
    },
    "bluetooth": {
      "format-on": "bt Ôäî",
      "format-off": "bt ({status}) Ôäî",
      "format-connected": "{device_alias} Ôäî",
      "format-connected-battery": "{device_alias} [{device_battery_percentage}%] Ôäî",
      "format-device-preference": ["ugreen_1", "ugreen_2"],
      "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
      "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
      "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
      "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%"
    },
    "hyprland/submap": {
      "format": "<span style=\"italic\">{}</span>"
    },
    "mpris": {
      "interval": 15,
      "format": "{player_icon} {dynamic}",
      "format-paused": "{status_icon} <i>{dynamic}</i>",
      "dynamic-order": ["artist", "title"],
      "player-icons": {
        "default": "‚ñ∂",
        "firefox": "‚ñ∂",
        "tidal-hifi": "üéµ",
        "cmus": "üéµ"
      },
      "status-icons": {
        "paused": "‚è∏"
      },
      "dynamic-len": 60
    },
    "idle_inhibitor": {
      "format": "{icon}",
      "format-icons": {
        "activated": "ÔÅÆ ",
        "deactivated": "ÔÅ∞ "
      }
    },
    "tray": {
      "icon-size": 13,
      "spacing": 10,
      "cursor": true
    },
    "clock": {
      "interval": 60,
      "timezone": "Asia/Jakarta",
      "format": "{:%a %d %b %y | %H:%M ÔÅ≥} ",
      "tooltip-format": "<tt>{calendar}</tt>",
      "calendar": {
        "mode": "year",
        "mode-mon-col": 3,
        "weeks-pos": "right",
        "on-scroll": 1,
        "on-click-right": "mode",
        "format": {
          "months": "<span color='#cba6f7'><b>{}</b></span>",
          "days": "<span color='#cdd6f4'><b>{}</b></span>",
          "weeks": "<span color='#94e2d5'>W{}</span>",
          "weekdays": "<span color='#f9e2af'><b>{}</b></span>",
          "today": "<span color='#f5e0dc'><b><u>{}</u></b></span>"
        }
      },
      "actions": {
        "on-click-right": "mode",
        "on-click-forward": "tz_up",
        "on-click-backward": "tz_down",
        "on-scroll-up": "shift_up",
        "on-scroll-down": "shift_down"
      }
    },
    "cpu": {
      "interval": 3,
      "format": "{usage}% Ôãõ ",
      "on-click": "missioncenter"
    },
    "memory": {
      "interval": 3,
      "format": "{used}GB ÓøÖ ",
      "on-click": "missioncenter",
      "tooltip-format": "Used: {used:0.1f}G/{total:0.1f}G. Swap: {swapUsed:0.1f}G/{swapTotal:0.1f}G",
      "states": {
        "critical": 80
      }
    },
    "temperature": {
      "interval": 3,
      "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
      "critical-threshold": 90,
      "format-critical": "{temperatureC}¬∞C {icon} ",
      "format": "{temperatureC}¬∞C {icon}",
      "format-icons": ["Ôù´", "Ôãâ", "Ôù©"]
    },
    "disk#ssd": {
      "interval": 60,
      "format": "{path} {free} ÔÇ† ",
      "path": "/",
      "tooltip": true,
      "warning": 80,
      "critical": 90
    },
    "network": {
      "interval": 1,
      "interface-ethernet": "enp5s*",
      "interface-wifi": "wlan0",
      "format-ethernet": "{bandwidthUpBytes}ÔÅ¢ {bandwidthDownBytes}ÔÅ£ ",
      "format-wifi": "{essid} ({signalStrength}%) Ôá´",
      "tooltip-format-ethernet": "{ifname}: {ipaddr}/{cidr} Ôûñ",
      "tooltip-format-wifi": "{ifname}: {ipaddr}/{cidr} Ôá´",
      "format-linked": "(No IP) Ôûñ",
      "format-disconnected": "Disconnected ‚ö†"
    },
    "custom/workspaces-mode": {
      "exec": "cat ~/.config/hypr/workspace_toggle_state | awk '{print toupper(substr($0,1,1)) substr($0,2)}'",
      "interval": 1,
      "format": "{} Û∞ï∞ ",
      "on-click": "~/script/workspace-toggle.sh"
    },
    "custom/audio-output": {
      "exec": "~/script/get-audio-device.sh",
      "interval": 5,
      "on-click": "~/script/audio-device-switch.sh"
    },
    "pulseaudio": {
      "scroll-step": 2,
      "format": "{volume}% {icon}ÔÅã {format_source}",
      "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
      "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
      "format-muted": "Ôö© {format_source}",
      "format-source": "{volume}% Ó¥É ",
      "format-source-muted": "ÔÑ±",
      "format-icons": {
        "headphone": "Ôñè ",
        "hands-free": "Ôñê",
        "headset": "Ôñê",
        "phone": "ÔÇï",
        "portable": "ÔÇï",
        "car": "ÔÜπ"
      },
      "on-click": "pavucontrol"
    }
  },
  {
    "output": "DP-3",
    "layer": "top",
    "position": "top",
    "height": 30,
    "width": 1920,
    "modules-left": ["hyprland/workspaces"],
    "hyprland/workspaces": {
      "format": "{id}"
    }
  },
  {
    "output": "DP-2",
    "layer": "top",
    "position": "top",
    "height": 30,
    "width": 1920,
    "modules-left": ["hyprland/workspaces"],
    "hyprland/workspaces": {
      "format": "{id}"
    }
  }
]
